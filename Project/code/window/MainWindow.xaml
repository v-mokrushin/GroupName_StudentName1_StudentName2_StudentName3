<Window
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:dataGrid2D="http://gu.se/DataGrid2D"
    xmlns:local="clr-namespace:Project"
    xmlns:System="clr-namespace:System;assembly=mscorlib" 
    xmlns:Themes="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Aero2" x:Class="Project.MainWindow"
    mc:Ignorable="d" 
    Title="" Height="750" Width="1150" 
    WindowStartupLocation="CenterScreen"
    ResizeMode="CanMinimize"
    Background="#fbfbfb">
    
    <Window.Resources>

        <System:Double x:Key="CommonFontSize">12</System:Double>
        <System:Double x:Key="SemiHeaderFontSize">18</System:Double>
        <System:Double x:Key="BlockHeaderFontSize">17</System:Double>
        <System:Double x:Key="HeaderFontSize">24</System:Double>
        <FontFamily x:Key="MainFont">Segoe UI</FontFamily>

        <Brush x:Key="ColorMainFont">#0a0a0a</Brush>
        <Brush x:Key="ColorGridBackground">#fbfbfb</Brush>
        <Brush x:Key="ColorBlockBackground">#eeebeb</Brush>
        <Brush x:Key="ColorBlockBackgroundMouseOn">#c9def5</Brush>

        <Style x:Key="HeaderInterfaceStyle">
            <Setter Property="Control.FontFamily" Value="{DynamicResource MainFont}"/>
            <Setter Property="Control.FontSize" Value="{DynamicResource HeaderFontSize}"/>
            <Setter Property="Control.Foreground" Value="{DynamicResource ColorMainFont}"/>
            <Setter Property="FrameworkElement.VerticalAlignment" Value="Center"/>
            <Setter Property="FrameworkElement.HorizontalAlignment" Value="Center"/>
        </Style>
        <Style x:Key="SemiHeaderInterfaceStyle">
            <Setter Property="Control.FontFamily" Value="{DynamicResource MainFont}"/>
            <Setter Property="Control.FontSize" Value="{DynamicResource SemiHeaderFontSize}"/>
            <Setter Property="Control.Foreground" Value="{DynamicResource ColorMainFont}"/>
            <Setter Property="FrameworkElement.VerticalAlignment" Value="Center"/>
            <Setter Property="FrameworkElement.HorizontalAlignment" Value="Left"/>
        </Style>
        <Style x:Key="SectionHeaderInterfaceStyle">
            <Setter Property="Control.FontFamily" Value="{DynamicResource MainFont}"/>
            <Setter Property="Control.FontSize" Value="{DynamicResource SemiHeaderFontSize}"/>
            <Setter Property="Control.Foreground" Value="{DynamicResource ColorMainFont}"/>
            <Setter Property="FrameworkElement.VerticalAlignment" Value="Center"/>
            <Setter Property="FrameworkElement.HorizontalAlignment" Value="Center"/>
        </Style>
        <Style x:Key="BlockHeaderInterfaceStyle">
            <Setter Property="Control.FontFamily" Value="{DynamicResource MainFont}"/>
            <Setter Property="Control.FontSize" Value="{DynamicResource BlockHeaderFontSize}"/>
            <Setter Property="Control.Foreground" Value="{DynamicResource ColorMainFont}"/>
            <Setter Property="FrameworkElement.VerticalAlignment" Value="Center"/>
            <Setter Property="FrameworkElement.HorizontalAlignment" Value="Left"/>
        </Style>
        <Style x:Key="CriteriaHeaderStyle">
            <Setter Property="Control.FontFamily" Value="{DynamicResource MainFont}"/>
            <Setter Property="Control.FontSize" Value="14"/>
            <Setter Property="Control.Foreground" Value="{DynamicResource ColorMainFont}"/>
            <Setter Property="FrameworkElement.VerticalAlignment" Value="Center"/>
            <Setter Property="FrameworkElement.HorizontalAlignment" Value="Center"/>
        </Style>
        <Style x:Key="CriteriaSectionsHeaderStyle">
            <Setter Property="Control.FontFamily" Value="{DynamicResource MainFont}"/>
            <Setter Property="Control.FontSize" Value="16"/>
            <Setter Property="Control.Foreground" Value="{DynamicResource ColorMainFont}"/>
            <Setter Property="FrameworkElement.VerticalAlignment" Value="Center"/>
            <Setter Property="FrameworkElement.HorizontalAlignment" Value="Center"/>
        </Style>

        <Style x:Key="CommonInterfaceStyle">
            <Setter Property="Control.FontFamily" Value="{DynamicResource MainFont}"/>
            <Setter Property="Control.FontSize" Value="{DynamicResource CommonFontSize}"/>
            <Setter Property="Control.Foreground" Value="{DynamicResource ColorMainFont}"/>
            <Setter Property="FrameworkElement.VerticalAlignment" Value="Center"/>
            <Setter Property="FrameworkElement.HorizontalAlignment" Value="Left"/>
        </Style>

        <Style x:Key="ButtonsInterfaceStyle">
            <Setter Property="Control.FontFamily" Value="{DynamicResource MainFont}"/>
            <Setter Property="Control.FontSize" Value="{DynamicResource CommonFontSize}"/>
            <Setter Property="Control.Foreground" Value="{DynamicResource ColorMainFont}"/>
            <Setter Property="FrameworkElement.Margin" Value="5px"/>
            <Setter Property="FrameworkElement.VerticalAlignment" Value="Stretch"/>
            <Setter Property="FrameworkElement.HorizontalAlignment" Value="Stretch"/>
        </Style>
        <Style x:Key="TextBoxStyle">
            <Setter Property="Control.Background" Value="Transparent"/>
            <Setter Property="Control.FontFamily" Value="{DynamicResource MainFont}"/>
            <Setter Property="Control.Foreground" Value="{DynamicResource ColorMainFont}"/>
            <Setter Property="Control.FontSize" Value="{DynamicResource CommonFontSize}"/>
            <Setter Property="TextBox.TextAlignment" Value="Left"/>
            <Setter Property="TextBox.HorizontalAlignment" Value="Stretch"/>
            <Setter Property="TextBox.VerticalAlignment" Value="Stretch"/>
            <Setter Property="TextBox.VerticalContentAlignment" Value="Center"/>
            <Setter Property="Control.BorderThickness" Value="1"/>
            <Setter Property="TextBox.TextWrapping" Value="NoWrap"/>
            <Setter Property="TextBoxBase.HorizontalScrollBarVisibility" Value="Auto"/>
        </Style>
        <Style x:Key="ButtonStyle">
            <Setter Property="Control.Background" Value="Transparent"/>
            <Setter Property="Control.FontFamily" Value="{DynamicResource MainFont}"/>
            <Setter Property="Control.Foreground" Value="{DynamicResource ColorMainFont}"/>
            <Setter Property="Control.FontSize" Value="{DynamicResource HeaderFontSize}"/>
        </Style>

        <Style x:Key="SectionHeadersStyle">
            <Setter Property="Control.Height" Value="35px"/>
            <Setter Property="Control.Background" Value="Transparent"/>
        </Style>
        <Style x:Key="XGridSectionHeaderStyle">
            <Setter Property="Control.Height" Value="60px"/>
            <Setter Property="Control.Background" Value="Transparent"/>
        </Style>
        <Style x:Key="XGridStyle">
            <Setter Property="Control.Height" Value="60px"/>
            <Setter Property="Control.Background" Value="{DynamicResource ColorBlockBackground}"/>
        </Style>
        <Style x:Key="XGridNewStyle">
            <Setter Property="Control.Background" Value="{DynamicResource ColorBlockBackground}"/>
        </Style>
        <Style x:Key="XShiftGridStyle">
            <Setter Property="Control.Height" Value="10px"/>
            <Setter Property="Control.Background" Value="Transparent"/>
        </Style>
        <Style x:Key="XShiftSectionStyle">
            <Setter Property="Control.Height" Value="20px"/>
            <Setter Property="Control.Background" Value="Transparent"/>
        </Style>

        <Style TargetType="TextBlock">
            <Setter Property="Control.FontFamily" Value="{DynamicResource MainFont}"/>
            <Setter Property="Control.FontSize" Value="{DynamicResource CommonFontSize}"/>
            <Setter Property="Control.Foreground" Value="{DynamicResource ColorMainFont}"/>
            <Setter Property="FrameworkElement.VerticalAlignment" Value="Center"/>
            <Setter Property="FrameworkElement.HorizontalAlignment" Value="Center"/>
        </Style>

    </Window.Resources>
    
    <Grid x:Name="gridRoot" Background="{DynamicResource ColorGridBackground}" Visibility="Visible">
        <Grid x:Name="gridStart" Visibility="Hidden">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="50px"/>
                <ColumnDefinition Width="10*"/>
                <ColumnDefinition Width="30px"/>
                <ColumnDefinition Width="10*"/>
                <ColumnDefinition Width="50px"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="10px"/>
                <RowDefinition Height="50px"/>
                <RowDefinition Height="15px"/>
                <RowDefinition Height="50px"/>
                <RowDefinition Height="20px"/>
                <RowDefinition Height="75px"/>
                <RowDefinition Height="10px"/>
                <RowDefinition Height="75px"/>
                <RowDefinition Height="10px"/>
                <RowDefinition Height="75px"/>
                <RowDefinition Height="10px"/>
            </Grid.RowDefinitions>
            <Label Content="Расчёт оценки защищенности ИС" Style="{DynamicResource HeaderInterfaceStyle}"
                   Grid.Column="1" Grid.Row="1"/>
            <Label Content="Начало работы" Style="{DynamicResource SemiHeaderInterfaceStyle}"
                   Grid.Column="1" Grid.Row="3"/>
            
            <Grid x:Name="gridStartOpenSolition"
                  Grid.Column="1" Grid.Row="5" Background="{DynamicResource ColorBlockBackground}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="10px"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="4*"/>
                    <ColumnDefinition Width="20px"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="5px"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="5px"/>
                </Grid.RowDefinitions>
                <Grid Grid.Column="1" Grid.Row="1" Grid.RowSpan="2">
                    <Image Source="pack://application:,,,/res/images/open_section_open.png" Margin="10"/>
                </Grid>
                <Label Content="Открыть проект" Style="{DynamicResource BlockHeaderInterfaceStyle}"
                   Grid.Column="2" Grid.Row="1"/>
                <Label Content="Откройте раннее созданный локальный проект" Style="{DynamicResource CommonInterfaceStyle}"
                   Grid.Column="2" Grid.Row="2"/>
            </Grid>
            <Grid x:Name="gridStartCreateSolition"
                  Grid.Column="1" Grid.Row="7" Background="{DynamicResource ColorBlockBackground}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="10px"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="4*"/>
                    <ColumnDefinition Width="20px"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="5px"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="5px"/>
                </Grid.RowDefinitions>
                <Grid Grid.Column="1" Grid.Row="1" Grid.RowSpan="2">
                    <Image Source="pack://application:,,,/res/images/open_section_create.png" Margin="10"/>
                </Grid>
                <Label Content="Создать проект" Style="{DynamicResource BlockHeaderInterfaceStyle}"
                   Grid.Column="2" Grid.Row="1"/>
                <Label Content="Создайте новый проект рассчета оценки защ. ИС" Style="{DynamicResource CommonInterfaceStyle}"
                   Grid.Column="2" Grid.Row="2"/>
            </Grid>
            <Grid x:Name="gridStartCopySolution" Visibility="Hidden"
                  Grid.Column="1" Grid.Row="9" Background="{DynamicResource ColorBlockBackground}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="10px"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="4*"/>
                    <ColumnDefinition Width="20px"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="5px"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="5px"/>
                </Grid.RowDefinitions>
                <Grid Grid.Column="1" Grid.Row="1" Grid.RowSpan="2">
                    <Image Source="pack://application:,,,/res/images/open_section_clone.png" Margin="10"/>
                </Grid>

                <Label Content="Клонировать проект" Style="{DynamicResource BlockHeaderInterfaceStyle}"
                   Grid.Column="2" Grid.Row="1"/>
                <Label Content="Создайте копию реннее созданого проекта" Style="{DynamicResource CommonInterfaceStyle}"
                   Grid.Column="2" Grid.Row="2"/>
            </Grid>
            <Grid x:Name="gridStartExitProgram"
                  Grid.Column="3" Grid.Row="7" Background="{DynamicResource ColorBlockBackground}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="10px"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="4*"/>
                    <ColumnDefinition Width="20px"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="5px"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="5px"/>
                </Grid.RowDefinitions>
                <Grid Grid.Column="1" Grid.Row="1" Grid.RowSpan="2">
                    <Image Source="pack://application:,,,/res/images/open_section_exit.png" Margin="10"/>
                </Grid>

                <Label Content="Выход" Style="{DynamicResource BlockHeaderInterfaceStyle}"
                   Grid.Column="2" Grid.Row="1"/>
                <Label Content="Завершите работу в программе" Style="{DynamicResource CommonInterfaceStyle}"
                   Grid.Column="2" Grid.Row="2"/>
            </Grid>
            <Grid x:Name="gridStartHelp" Visibility="Visible"
                  Grid.Column="1" Grid.Row="9" Background="{DynamicResource ColorBlockBackground}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="10px"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="4*"/>
                    <ColumnDefinition Width="20px"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="5px"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="5px"/>
                </Grid.RowDefinitions>
                <Grid Grid.Column="1" Grid.Row="1" Grid.RowSpan="2">
                    <Image Source="pack://application:,,,/res/images/section_help.png" Margin="10"/>
                </Grid>

                <Label Content="Помощь" Style="{DynamicResource BlockHeaderInterfaceStyle}"
                   Grid.Column="2" Grid.Row="1"/>
                <Label Content="Узнать больше о программе" Style="{DynamicResource CommonInterfaceStyle}"
                   Grid.Column="2" Grid.Row="2"/>
            </Grid>
            <Grid x:Name="gridStartSettings" Visibility="Hidden"
                  Grid.Column="3" Grid.Row="9" Background="{DynamicResource ColorBlockBackground}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="10px"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="4*"/>
                    <ColumnDefinition Width="20px"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="5px"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="5px"/>
                </Grid.RowDefinitions>
                <Grid Grid.Column="1" Grid.Row="1" Grid.RowSpan="2">
                    <Image Source="pack://application:,,,/res/images/open_section_settings.png" Margin="10"/>
                </Grid>

                <Label Content="Настройки" Style="{DynamicResource BlockHeaderInterfaceStyle}"
                   Grid.Column="2" Grid.Row="1"/>
                <Label Content="" Style="{DynamicResource HeaderInterfaceStyle}"
                   Grid.Column="2" Grid.Row="2"
                   HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="12"/>
            </Grid>
            <Grid x:Name="gridChangeTheme" Visibility="Visible"
                  Grid.Column="3" Grid.Row="5" Background="{DynamicResource ColorBlockBackground}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="10px"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="4*"/>
                    <ColumnDefinition Width="20px"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="5px"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="5px"/>
                </Grid.RowDefinitions>
                <Grid Grid.Column="1" Grid.Row="1" Grid.RowSpan="2">
                    <Image Source="pack://application:,,,/res/images/open_section_settings.png" Margin="10"/>
                </Grid>

                <Label Content="Тема" Style="{DynamicResource BlockHeaderInterfaceStyle}"
                   Grid.Column="2" Grid.Row="1"/>
                <Label Content="Смените цветовую тему" Style="{DynamicResource HeaderInterfaceStyle}"
                   Grid.Column="2" Grid.Row="2"
                   HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="12"/>
            </Grid>
        </Grid>
        <Grid x:Name="gridSolutionCreate" Visibility="Visible">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="50px"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="20px"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="50px"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="10px"/>
                <RowDefinition Height="50px"/>
                <RowDefinition Height="15px"/>
                <RowDefinition Height="50px"/>
                <RowDefinition Height="20px"/>
                <RowDefinition Height="75px"/>
                <RowDefinition Height="10px"/>
                <RowDefinition Height="75px"/>
                <RowDefinition Height="10px"/>
                <RowDefinition Height="75px"/>
                <RowDefinition Height="10px"/>
                <RowDefinition Height="45px"/>
                <RowDefinition Height="10px"/>
            </Grid.RowDefinitions>

            <Label x:Name="labelCreateTitle" Content="Создание проекта &quot;Project&quot;" Style="{DynamicResource HeaderInterfaceStyle}"
                   Grid.Column="1" Grid.Row="1"
                   HorizontalAlignment="Left"/>
            <Label Content="Введите данные" Style="{DynamicResource SemiHeaderInterfaceStyle}"
                   Grid.Column="1" Grid.Row="3"/>
            <Label Content="Результат" Style="{DynamicResource SemiHeaderInterfaceStyle}"
                   Grid.Column="3" Grid.Row="3"/>
            <Grid x:Name="gridCreateName"
                  Grid.Column="1" Grid.Row="5" Background="{DynamicResource ColorBlockBackground}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="10px"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="4*"/>
                    <ColumnDefinition Width="20px"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="5px"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="5px"/>
                </Grid.RowDefinitions>
                <Grid Grid.Column="1" Grid.Row="1" Grid.RowSpan="2">
                    <Image Source="pack://application:,,,/res/images/create_section_name.png" Margin="10"/>
                </Grid>
                <Label Content="Задайте имя проекту" Style="{DynamicResource BlockHeaderInterfaceStyle}"
                   Grid.Column="2" Grid.Row="1"/>
                <TextBox x:Name="textBoxProjectName" Style="{DynamicResource TextBoxStyle}" Text=""
                   Grid.Column="2" Grid.Row="2"/>
            </Grid>
            <Grid x:Name="gridCreatePath"
                  Grid.Column="1" Grid.Row="9" Background="{DynamicResource ColorBlockBackground}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="10px"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="4*"/>
                    <ColumnDefinition Width="20px"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="5px"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="5px"/>
                </Grid.RowDefinitions>
                <Grid Grid.Column="1" Grid.Row="1" Grid.RowSpan="2">
                    <Image Source="pack://application:,,,/res/images/create_section_path.png" Margin="10"/>
                </Grid>
                <Label Content="Укажите путь" Style="{DynamicResource BlockHeaderInterfaceStyle}"
                   Grid.Column="2" Grid.Row="1" Grid.ColumnSpan="3"/>
                <Label x:Name="labelProjectPath" Content="" Style="{DynamicResource CommonInterfaceStyle}"
                   Grid.Column="2" Grid.Row="2"/>
            </Grid>
            <Grid x:Name="gridCreateComment"
                  Grid.Column="1" Grid.Row="7" Background="{DynamicResource ColorBlockBackground}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="10px"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="4*"/>
                    <ColumnDefinition Width="20px"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="5px"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="5px"/>
                </Grid.RowDefinitions>
                <Grid Grid.Column="1" Grid.Row="1" Grid.RowSpan="2">
                    <Image Source="pack://application:,,,/res/images/create_section_comment.png" Margin="10"/>
                </Grid>
                <Label Content="Введите комментарий" Style="{DynamicResource BlockHeaderInterfaceStyle}"
                   Grid.Column="2" Grid.Row="1"/>
                <TextBox x:Name="textBoxProjectComment" Style="{DynamicResource TextBoxStyle}" Text=""
                   Grid.Column="2" Grid.Row="2"/>
            </Grid>
            <Grid x:Name="gridCreateResult"
                  Grid.Column="3" Grid.Row="5" Grid.RowSpan="5" Background="{DynamicResource ColorBlockBackground}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="10px"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="1px"/>
                    <ColumnDefinition Width="10px"/>
                    <ColumnDefinition Width="2*"/>
                    <ColumnDefinition Width="10px"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="8px"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="8px"/>
                </Grid.RowDefinitions>

                <Grid Grid.Column="2" Grid.Row="1" Grid.RowSpan="6" Background="Gray">

                </Grid>
                <Label Content="Название проекта" Style="{DynamicResource CommonInterfaceStyle}"
                   Grid.Column="1" Grid.Row="1"/>
                <Label Content="Путь к проекту" Style="{DynamicResource CommonInterfaceStyle}"
                   Grid.Column="1" Grid.Row="5"/>
                <Label Content="Комментарий" Style="{DynamicResource CommonInterfaceStyle}"
                   Grid.Column="1" Grid.Row="6"/>
                <Label Content="Дата создания" Style="{DynamicResource CommonInterfaceStyle}"
                   Grid.Column="1" Grid.Row="3"/>
                <Label Content="Время создания" Style="{DynamicResource CommonInterfaceStyle}"
                   Grid.Column="1" Grid.Row="4"/>
                <Label Content="Пользователь" Style="{DynamicResource CommonInterfaceStyle}"
                   Grid.Column="1" Grid.Row="2"/>

                <Label x:Name="labelNameSolutionCreate" Content="" Style="{DynamicResource CommonInterfaceStyle}"
                   Grid.Column="4" Grid.Row="1"/>
                <Label x:Name="labelPathSolutionCreate" Content="" Style="{DynamicResource CommonInterfaceStyle}"
                   Grid.Column="4" Grid.Row="5"/>
                <Label x:Name="labelUserSolutionCreate" Content="" Style="{DynamicResource CommonInterfaceStyle}"
                   Grid.Column="4" Grid.Row="2"/>
                <Label x:Name="labelDateSolutionCreate" Content="" Style="{DynamicResource CommonInterfaceStyle}"
                   Grid.Column="4" Grid.Row="3"/>
                <Label x:Name="labelTimeSolutionCreate" Content="" Style="{DynamicResource CommonInterfaceStyle}"
                   Grid.Column="4" Grid.Row="4"/>
                <Label x:Name="labelCommentSolutionCreate" Content="" Style="{DynamicResource CommonInterfaceStyle}"
                   Grid.Column="4" Grid.Row="6"/>
            </Grid>
            <Grid x:Name="gridCreateFinish"
                  Grid.Column="1" Grid.Row="11" Grid.ColumnSpan="3" Background="{DynamicResource ColorBlockBackground}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="20px"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="20px"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="5px"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="5px"/>
                </Grid.RowDefinitions>

                <Label Content="Готово" Style="{DynamicResource BlockHeaderInterfaceStyle}"
                   Grid.Column="1" Grid.Row="1" HorizontalAlignment="Center"/>

            </Grid>

        </Grid>
        <Grid x:Name="gridSolution" Visibility="Hidden">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="50px"/>
                <ColumnDefinition Width="395px"/>
                <ColumnDefinition Width="10px"/>
                <ColumnDefinition Width="1px"/>
                <ColumnDefinition Width="10px"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="50px"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="10px"/>
                <RowDefinition Height="50px"/>
                <RowDefinition Height="15px"/>
                <RowDefinition Height="50px"/>
                <RowDefinition Height="20px"/>
                <RowDefinition Height="75px"/>
                <RowDefinition Height="10px"/>
                <RowDefinition Height="75px"/>
                <RowDefinition Height="10px"/>
                <RowDefinition Height="75px"/>
                <RowDefinition Height="10px"/>
                <RowDefinition Height="1px"/>
                <RowDefinition Height="10px"/>
                <RowDefinition Height="75px"/>
                <RowDefinition Height="10px"/>
                <RowDefinition Height="32px"/>
                <RowDefinition Height="10px"/>
                <RowDefinition Height="32px"/>
                <RowDefinition Height="10px"/>
                <RowDefinition Height="1px"/>
                <RowDefinition Height="10px"/>
                <RowDefinition Height="75px"/>
                <RowDefinition Height="40px"/>
            </Grid.RowDefinitions>

            <Label x:Name="labelSolutionTitle" Content="Проект" Style="{DynamicResource HeaderInterfaceStyle}"
                   Grid.Column="1" Grid.Row="1" HorizontalAlignment="Left"/>
            <Label Content="Меню" Style="{DynamicResource SemiHeaderInterfaceStyle}"
                   Grid.Column="1" Grid.Row="3"/>
            <Label x:Name="labelSolutionSectionView" Content="Введите значения критериев" Style="{DynamicResource SemiHeaderInterfaceStyle}"
                   Grid.Column="5" Grid.Row="3"/>
            <Grid Grid.Column="3" Grid.Row="5" Grid.RowSpan="17" Background="Gray"></Grid>
            <Grid Grid.Column="1" Grid.Row="11" Background="Gray"></Grid>
            <Grid Grid.Column="1" Grid.Row="19" Background="Gray"></Grid>
            <Grid x:Name="gridSolutionInput"
                  Grid.Column="1" Grid.Row="5" Background="{DynamicResource ColorBlockBackground}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="10px"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="4*"/>
                    <ColumnDefinition Width="20px"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="5px"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="5px"/>
                </Grid.RowDefinitions>
                <Grid Grid.Column="1" Grid.Row="1" Grid.RowSpan="2">
                    <Image Source="pack://application:,,,/res/images/solution_section_input.png" Margin="10"/>
                </Grid>

                <Label Content="Раздел &quot;Введение данных&quot;" Style="{DynamicResource BlockHeaderInterfaceStyle}"
                   Grid.Column="2" Grid.Row="1"/>
                <Label Content="Введите данные, чтобы сформировать отчет" Style="{DynamicResource CommonInterfaceStyle}"
                   Grid.Column="2" Grid.Row="2"/>
            </Grid>
            <Grid x:Name="gridSolutionReport"
                  Grid.Column="1" Grid.Row="7" Background="{DynamicResource ColorBlockBackground}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="10px"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="4*"/>
                    <ColumnDefinition Width="20px"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="5px"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="5px"/>
                </Grid.RowDefinitions>
                <Grid Grid.Column="1" Grid.Row="1" Grid.RowSpan="2">
                    <Image Source="pack://application:,,,/res/images/solution_section_report.png" Margin="10"/>
                </Grid>

                <Label Content="Раздел &quot;Формирование отчета&quot;" Style="{DynamicResource BlockHeaderInterfaceStyle}"
                   Grid.Column="2" Grid.Row="1"/>
                <Label Content="Перейдите, когда закончите с введением данных" Style="{DynamicResource CommonInterfaceStyle}"
